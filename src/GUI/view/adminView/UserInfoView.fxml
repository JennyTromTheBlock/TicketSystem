<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="600.0" prefWidth="394.0" spacing="40.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controller.adminControllers.UserInfoController">
   <children>
      <Label fx:id="lblName" alignment="CENTER" layoutX="30.0" layoutY="50.0" prefHeight="59.0" prefWidth="354.0">
         <font>
            <Font size="25.0" />
         </font>
         <VBox.margin>
            <Insets top="100.0" />
         </VBox.margin></Label>
      <Label fx:id="lblEmail" alignment="CENTER" contentDisplay="CENTER" layoutX="30.0" layoutY="50.0" prefHeight="87.0" prefWidth="354.0" text="Select User For More Info">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Label fx:id="lblEventSOnUsers" alignment="CENTER" layoutX="30.0" layoutY="150.0" prefHeight="59.0" prefWidth="354.0" text="list of event conncted to user:" visible="false">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <ListView fx:id="listviewAssignedEvents" prefHeight="200.0" prefWidth="200.0" visible="false" />
      <Pane prefHeight="163.0" prefWidth="354.0" VBox.vgrow="ALWAYS" />
      <HBox alignment="CENTER" prefHeight="106.0" prefWidth="354.0">
         <children>
            <Button fx:id="btnDeleteUser" mnemonicParsing="false" onAction="#handleDeleteUser" prefHeight="55.0" prefWidth="191.0" text="Delete User" visible="false" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="30.0" left="20.0" right="20.0" top="40.0" />
   </padding>
</VBox>
