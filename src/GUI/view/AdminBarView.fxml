<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="GUI.controller.adminControllers.AdminBarView"
      prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button fx:id="btnCreateUser" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#handleCreateUser" prefHeight="90.0" prefWidth="270.0" text="Create User">
            <font>
                <Font name="Berlin Sans FB" size="36.0" />
            </font>
        </Button>
        <Button fx:id="btnSeeEvents" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#handleSeeEvents" prefHeight="90.0" prefWidth="270.0" text="See Events">
            <font>
                <Font name="Berlin Sans FB" size="36.0" />
            </font>
        </Button>
        <Button fx:id="btnSeeUsers" layoutX="311.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#handleSeeUsers" prefHeight="90.0" prefWidth="270.0" text="See Users">
            <font>
                <Font name="Berlin Sans FB" size="36.0" />
            </font>
        </Button>
    </children>
    <VBox.margin>
        <Insets bottom="20.0" />
    </VBox.margin>
</HBox>
