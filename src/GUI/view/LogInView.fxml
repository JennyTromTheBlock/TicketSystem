<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controller.LogInController">
   <children>
      <Label fx:id="sideField" alignment="CENTER" contentDisplay="CENTER" layoutY="-1.0" prefHeight="400.0" prefWidth="243.0" />
      <ImageView fx:id="logoImg" fitHeight="81.0" fitWidth="200.0" layoutX="22.0" layoutY="144.0" pickOnBounds="true" preserveRatio="true" />
      <VBox alignment="TOP_CENTER" layoutX="246.0" layoutY="34.0" prefHeight="367.0" prefWidth="354.0">
         <children>
            <Label text="Log In">
               <font>
                  <Font size="40.0" />
               </font>
            </Label>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="46.0" prefWidth="191.0" text="Log in to your account" />
            <VBox>
               <VBox.margin>
                  <Insets bottom="50.0" top="20.0" />
               </VBox.margin>
               <children>
                  <TextField prefHeight="26.0" prefWidth="172.0" promptText="Username">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin></TextField>
               </children>
            </VBox>
            <TextField promptText="Password">
               <VBox.margin>
                  <Insets bottom="50.0" />
               </VBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#logInBtn" text="Log In">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin></Button>
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
